  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!axios|@testing-library/dom|@fortawesome/fontawesome-svg-core|@fortawesome/react-fontawesome|@fortawesome/free-solid-svg-icons)"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "react-scripts": "^5.0.1"
  }
